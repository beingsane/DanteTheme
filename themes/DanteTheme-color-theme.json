	"tokenColors": [
		{
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#1900ff"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#898984"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#ff7a68"
			}
		},
		{
			"name": "Doc String",
			"scope": "string.quoted.docstring.multi.python",
			"settings": {
				"foreground": "#898984"
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#0051ff"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#0051ff"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#6ab3aa"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#fbd964"
			}
		},
		{
			"name": "Numeric suffix",
			"scope": "numeric-suffix",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0004fa"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#cd316e"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#f2f3cb"
			}
		},
		{
			"name": "Punctuation tilde ~",
			"scope": "punctuation.tilde",
			"settings": {
				"fontStyle": "",
				"foreground": "#0004ff"
			}
		},
		{
			"name": "Punctuation brackets attribute",
			"scope": "punctuation.squarebracket-attribute",
			"settings": {
				"fontStyle": "",
				"foreground": "#0004ff"
			}
		},
		{
			"name": "Keyword type",
			"scope": "keyword.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f700ff"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#b1ddf6"
			}
		},
		{
			"name": "Keyword other class",
			"scope": "keyword.other.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff00c8"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#00ffb3"
			}
		},
		{
			"name": "Function call",
			"scope": "meta.function-call",
			"settings": {
				"fontStyle": "",
				"foreground": "#34a3f5"
			}
		},
		{
			"name": "Meta function call and type-parameter",
			"scope": "storage.type, entity.name.type.type-parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#66D9EF"
			}
		},
		{
			"name": "La f del 10.0f",
			"scope": "storage.type.numeric",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff0080"
			}
		},
